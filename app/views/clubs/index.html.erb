Clubs
<% @clubs.each_with_index do |club, index| %>
  <p>#<%= index + 1 %> | Name: <%= club.name %>, Semester: <%= club.semester.session %>, 
  	  <%= link_to 'View Club Details', club_path(club), id: "club_#{club.id}" %> | 
  	  <%= link_to 'View Club Activity Profile', activities_path(club), id: "activity_club_#{club.id}" %> |
  	  <%= link_to 'View Member', members_club_path(club), id: "member_club_#{club.id}" %>
  	  	
  </p>
<% end %>